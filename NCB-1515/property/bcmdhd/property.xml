<?xml version="1.0" encoding="iso-8859-1"?>
<!-- property sub-directory -->
<property>
    <scope>property</scope>
    <description>bcmdhd driver for AP12356(Wifi)</description>
    <default_option>v1.0</default_option>
    <opt>
    	<!-- option: opt option -->
        <option>v1.0</option>

        <!-- Description: put your version description here -->
        <Description>Initial version</Description>

        <!-- src_dir: offset related to <project_name>/property -->
        <src_dir>bcmdhd/v1.0</src_dir>

	<!-- howto: how to build source code -->
	<howto>make clean; make</howto>

        <!-- target: multiple define is valid, sh as example -->
	<target type="drv">
	    <!-- target_in_src_file: shell script filename -->
	    <offset_in_src_dir>.</offset_in_src_dir>

	    <!-- filename: filename -->
	    <filename>bcmdhd.ko</filename>

            <!-- target_subdir: sub-dir related to target  -->
	    <target_subdir>Test/</target_subdir>

            <!-- target_order: executing order of this script -->
	    <target_order></target_order>
	</target>
        <!-- target: multiple define is valid, sh as example -->
	<target type="drv">
	    <!-- target_in_src_file: shell script filename -->
	    <offset_in_src_dir>../../../../../../../usr/src/linux/modules</offset_in_src_dir>

	    <!-- filename: filename -->
	    <filename>cfg80211.ko</filename>

            <!-- target_subdir: sub-dir related to target  -->
	    <target_subdir>Test/</target_subdir>

            <!-- target_order: executing order of this script -->
	    <target_order></target_order>
	</target>
       <!-- target: multiple define is valid, sh as example -->
	<target type="sh">
	    <!-- target_in_src_file: shell script filename -->
	    <offset_in_src_dir>../</offset_in_src_dir>

	    <!-- filename: filename -->
	    <filename>init_bcmdhd.sh</filename>

            <!-- target_subdir: sub-dir related to target  -->
	    <target_subdir>Test/</target_subdir>

            <!-- target_order: executing order of this script -->
	    <target_order></target_order>
	</target>
       <!-- target: multiple define is valid, sh as example -->
                 <target type="sh">
            <!-- target_in_src_file: shell script filename -->
            <offset_in_src_dir>../</offset_in_src_dir>

            <!-- filename: filename -->
            <filename>AP12356_wifi_test.sh</filename>

            <!-- target_subdir: sub-dir related to target  -->
            <target_subdir>Test/</target_subdir>

            <!-- target_order: executing order of this script -->
            <target_order>78a</target_order>
        </target>

       <!-- target: multiple define is valid, sh as example -->
	<target type="bin">
	    <!-- target_in_src_file: shell script filename -->
	    <offset_in_src_dir>../</offset_in_src_dir>

	    <!-- filename: filename -->
	    <filename>firmware1.tgz</filename>

