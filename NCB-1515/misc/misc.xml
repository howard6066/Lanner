        <Desc>Status LED Test</Desc>
            <!-- target: multiple define is valid, sh as example -->
	    <target type="sh">
	        <!-- target_in_src_file: shell script filename -->
	        <offset_in_src_dir>./../common</offset_in_src_dir>

	        <!-- filename: filename -->
	        <filename>sled_tst.sh</filename>

	        <!-- target_subdir: sub-dir related to target  -->
	        <target_subdir>Test/</target_subdir>

                <!-- target_order: executing order of this script -->
	        <target_order>54a</target_order>

	    </target>
            <!-- target: multiple define is valid, elf as example -->
	    <target type="bin">
		<!-- target_in_src_file: shell script filename -->
